<mjml>
    <mj-head>
        <mj-title>Game of the Week</mj-title>
        <mj-font name="Poppins"
                 href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap"/>
        <mj-font name="Manrope" href="https://fonts.googleapis.com/css2?family=Manrope:wght@200..800&display=swap"/>
        <mj-attributes>
            <mj-all font-family="Poppins, Arial" font-size="16px" font-weight="400" line-height="24px" color="#42474b"/>
        </mj-attributes>
        <mj-style>
            @font-face {
            font-family  : 'EuclidFlex';
            font-style   : normal;
            font-weight  : 400;
            font-display : swap;
            src          : url("https://ammarhabib.me/demo/assets/fonts/EuclidFlexRegular.ttf") format("truetype");
        }

        @font-face {
            font-family  : 'EuclidFlex';
            font-style   : normal;
            font-weight  : 700;
            font-display : swap;
            src          : url("https://ammarhabib.me/demo/assets/fonts/EuclidFlexBold.ttf") format("truetype");
        } </mj-style>
        <mj-style>
           
        getReadyContent {
            font-size   : 24px;
            line-height : 1.33;
        }

        
        

        @media only screen and (max-width : 480px) {
            .headingThree {
                font-size : 18px;
            }

            .getReadyContent {
                font-size   : 18px;
                line-height : 1.3;
            }

            .playBtn a {
                font-size : 24px !important;
                padding   : 16px 30px !important;
            }

            .socialItem td {
                padding : 4px !important;
            }

            .mainWrapper > table > tbody > tr > td {
                padding : 0 16px !important;
            }

            .gamesList .mj-column-per-25 {
                width         : 50% !important;
                margin-bottom : 12px;
            }

            .gamesList__img td {
                width : 100% !important
            }
        }

        @media only screen and (max-width : 425px) {
            .mainHeading {
                font-size   : 120px;
                line-height : 100px;
            }

            .ofThe {
                font-size : 100px;
            }

            .headingThree {
                font-size : 16px;
            }

            .headingTwo {
                font-size : 40px;
            }
        }

        @media only screen and (max-width : 375px) {
            .mainHeading {
                font-size   : 100px;
                line-height : 80px;
            }

            .ofThe {
                font-size : 85px;
            }

            .headingTwo {
                font-size : 35px;
            }

            .headingThree {
                font-size : 14px;
            }

            .socialItem td {
                padding : 2px !important;
            }

            .getReadyContent {
                font-size : 16px;
            }
        }

        @media only screen and (max-width : 320px) {
            .mainHeading {
                font-size   : 80px;
                line-height : 70px;
            }

            .ofThe {
                font-size : 80px;
            }

            .headingTwo {
                font-size   : 30px;
                line-height : 45px;
            }

            .headingThree {
                font-size : 12px;
            }

            .socialItem td {
                padding : 2px !important;
            }

            .getReadyContent {
                font-size : 14px;
            }

            .playBtn a {
                font-size   : 20px !important;
                padding     : 16px 18px !important;
                line-height : 12px;
            }
        }
        </mj-style>
    </mj-head>
    <mj-body>
        <mj-wrapper css-class="mainWrapper" full-width padding="0 30px" background-color="#000">
            <mj-section padding="35px 0 30px">
                <mj-column>
                    <mj-text padding="0" align="center" text-transform="uppercase"
                             font-family="'EuclidFlex','Manrope', Arial, sans-serif" color="#e51e1f" font-weight="700">
                        <div class="mainHeading">
                            <span style="display: block">Game</span>
                            <span class="ofThe" style="color:#fff; display: block">of the</span>
                            Week
                        </div>
                    </mj-text>
                </mj-column>
            </mj-section>
            <mj-section padding="20px 0 0">
                <mj-group>
                    <mj-column width="65%" vertical-align="middle">
