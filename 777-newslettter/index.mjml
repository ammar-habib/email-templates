<mjml>
    <mj-head>
        <mj-title>Game of the year</mj-title>
        <mj-font name="Poppins"
                 href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap"/>
        <mj-font name="Inter" href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;700&display=swap"/>
        <mj-attributes>
            <mj-all font-family="Poppins, Arial" font-size="16px" font-weight="400" line-height="24px" color="#42474b"/>
            <mj-class name="heading" font-weight="600" font-size="22px" line-height="30px"/>
            <mj-class name="btn" font-weight="400" font-size="16px" line-height="24px" background-color="#ffffff"
                      border="1px solid #D6D6D8" border-radius="0" inner-padding="8px 22px" color="#221f20"/>
        </mj-attributes>
        <mj-style>
            <mj-style> @font-face {
                font-family  : "markpro";
                font-style   : normal;
                font-weight  : 400;
                font-display : swap;
                src          : url("http://show-demo.herokuapp.com/found-factorty/fonts/MarkPro.ttf") format("truetype");
            } </mj-style>
        </mj-style>
    </mj-head>
    <mj-body>
        <mj-wrapper full-width padding="0 40px" background-color="#000">
            <mj-section padding="65px 0 85px">
                <mj-column>
                    <mj-text padding="0" align="center" text-transform="uppercase" font-family="po, Arial"
                             font-weight="400" font-size="143px" line-height="123px" color="#e51e1f">Game<br>
